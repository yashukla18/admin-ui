    "@babel/preset-typescript": "^7.23.3",
    "@mui/types": "^7.2.13",
    "@originjs/vite-plugin-federation": "^1.3.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@tanstack/eslint-plugin-query": "^5.28.11",
