{
  "name": "@youscience/admin-ui",
  "version": "1.20.0",
  "description": "Frontend for Admin UI",
  "type": "module",
  "scripts": {
    "build:dev": "NODE_OPTIONS=--max_old_space_size=4096 rimraf build && vite build --mode development",
    "build:production": "NODE_OPTIONS=--max_old_space_size=4096 rimraf build && vite build --mode production",
    "build:sandbox": "NODE_OPTIONS=--max_old_space_size=4096 rimraf build && vite build --mode sandbox",
    "build:stage": "NODE_OPTIONS=--max_old_space_size=4096 rimraf build && vite build --mode stage",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 rimraf build && tsc && vite build",
    "dev": "vite",
    "development": "vite --mode development",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "lint-staged": "npm run lint-fix && npm run pretty",
    "local": "NODE_ENV=development vite --mode development",
    "prepare": "husky install",
    "pretty": "prettier --write ./src",
    "preview:dev": "vite preview --mode=development",
    "preview": "vite preview",
    "sandbox": "vite --mode sandbox",
    "stage": "vite --mode stage",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit --skipLibCheck -p ./tsconfig.json"
  },
  "keywords": [],
  "author": "YouScience Devs",
  "license": "MIT",
