    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@mui/system": "^5.15.7",
    "@mui/utils": "^5.15.7",
    "@mui/x-data-grid": "^6.19.6",
    "@mui/x-date-pickers": "^6.19.3",
    "@tanstack/react-query": "^5.32.0",
    "@types/react-gtm-module": "^2.0.3",
    "@types/react-select-country-list": "^2.2.3",
    "@youscience/assets": "^1.5.2",
    "@youscience/brightpath-header": "2.2.6",
    "@youscience/core": "2.0.4",
    "@youscience/hooks": "^1.3.2",
    "@youscience/theme": "1.2.5",
    "@youscience/user-service-common": "0.0.137",
    "axios": "^1.6.7",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
